{
  "manifest_version": 3,
  "name": "Secrets Manager",
  "version": "1.0.0",
  "description": "Store and manage your secrets with CSV import/export",
  "permissions": [
    "storage"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
